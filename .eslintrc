{
  "parser": "babel-eslint",
  "env": {
    "es2021": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "arrow-parens": "off",
    "require-jsdoc": "off",
    "operator-linebreak": "off"
  },
  "extends": [
    "eslint:recommended",
    "google"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  }
}